{
  "build": {
    "env": {
      "GATSBY_FATHOM_TRACKING_ID_SCOTTSPENCE_COM": "@gatsby_fathom_tracking_id_scottspence_com",
      "GATSBY_FATHOM_TRACKING_URL_SCOTTSPENCE_COM": "@gatsby_fathom_tracking_url_scottspence_com",
      "GATSBY_GITHUB_TOKEN": "@gatsby_github_token"
    }
  },
  "routes": [
    { "src": "/uses", "dest": "/#uses" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ]
}
